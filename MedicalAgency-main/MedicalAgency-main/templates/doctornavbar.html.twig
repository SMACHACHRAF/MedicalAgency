<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>{% block title %}Sidebar{% endblock %}</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('Admin_Dashboard/assets/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i">
        <link rel="stylesheet" href="{{asset('Admin_Dashboard/assets/css/ready.css') }}">
        <link rel="stylesheet" href="{{asset('Admin_Dashboard/assets/css/demo.css')}}">
    {% endblock %}
</head>
{% block navbar %}
    <body>
    <div class="wrapper">
        <div class="main-header">
            <div class="logo-header">
                <a href="index.html" class="logo">
                    Dotor Dashboard
                </a>
                <button class="navbar-toggler sidenav-toggler ml-auto" type="button" data-toggle="collapse" data-target="collapse" aria-controls="sidebar" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <button class="topbar-toggler more"><i class="la la-ellipsis-v"></i></button>
            </div>
            <nav class="navbar navbar-header navbar-expand-lg">
                <div class="container-fluid">

                    <form class="navbar-left navbar-form nav-search mr-md-3" action="">
                        <div class="input-group">
                            <input type="text" placeholder="Search ..." class="form-control">
                            <div class="input-group-append">
                                <span class="input-group-text">
									<i class="la la-search search-icon"></i>
								</span>
                            </div>
                        </div>
                    </form>
                    <ul class="navbar-nav topbar-nav ml-md-auto align-items-center">
                        <li class="nav-item dropdown">
                            <a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="#" aria-expanded="false"> <img src="{{asset('icons/doctor.png')}}" alt="user-img" width="36" class="img-circle"></span>
                            </a>
                            <ul class="dropdown-menu dropdown-user">
                                <a class="dropdown-item" href="{{ path('app_logout') }}"><i class="fa fa-power-off"></i> Logout</a>
                            </ul>
                            <!-- /.dropdown-user -->
                        </li>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>

        {% block test %}
        {% endblock %}
    </div>


    </body>
{% endblock %}
{% block js %}
    <script src="{{asset('Admin_Dashboard/assets/js/core/jquery.3.2.1.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/core/popper.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/core/bootstrap.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/chartist/chartist.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/chartist/plugin/chartist-plugin-tooltip.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/jquery-mapael/jquery.mapael.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/jquery-mapael/maps/world_countries.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/chart-circle/circles.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/ready.min.js') }}"></script>
    <script src="{{asset('Admin_Dashboard/assets/js/demo.js') }}"></script>
{% endblock %}
</html>
