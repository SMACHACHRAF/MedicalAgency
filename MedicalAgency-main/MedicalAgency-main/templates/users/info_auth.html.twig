{% extends 'emailtemp.html.twig' %}
{% block head_css %}
    <link rel="stylesheet" href="{{ asset('medicalcenter/assets/css/bootstrap.min.css') }}">{% endblock %}
{% block title %}Request{% endblock %}

{% block test %}

    <h2>Login Informations</h2>

    <p>Hello Doctor {{ name }},</p>
    <p>Your login informations are ready</p>
    <h5>Email</h5>
    <p>{{ email }} </p>
    <h5>Password</h5>
    <p>{{ pass }} </p>
    <p style="font-family: sans-serif; font-size: 14px; font-weight: normal; margin: 0; Margin-bottom: 15px;" >You can login from the website login page thank you</p>
{% endblock %}


